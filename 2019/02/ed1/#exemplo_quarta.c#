#include "stdio.h"



enum{false,true};

typedef struct {
  char nome[50];
  int matricula;
  float nota1;
  float nota2;

}Aluno;



int compare(char *a, char *b,int tamanho){
  int i=0;

  while(i<tamanho){
    if(a[i] != b[i]) {
      return false;     
    }
    i++;
  }
  return true;
}


Aluno ler(Aluno a);
int main(){

  Aluno alunos[3];
  int i=0;

  while( i < 3){
    alunos[i] = ler(alunos[i]); 

    i++;
  }
  i=0;
  while(i < 3){
    printf("O Aluno    %s    teve a mÃ©dia %.2f \n",alunos[i].nome, (alunos[i].nota1 + alunos[i].nota2)/2 );

    i++;}
  
}
Aluno ler(Aluno a){
  printf("Informe  o nome \n");
  scanf("%s",&a.nome);
  printf("Informe a matricula \n");
  scanf("%d",&a.matricula);
  printf("Informe a primeira nota \n");
  scanf("%f",&a.nota1);
  printf("Informe a segunda nota \n");
  scanf("%f",&a.nota2);

  char v1[20];
  char v2[20];



  if(compare(v1,v2,20)){ //  0 false 1 verdade

    }

  return a;

}
